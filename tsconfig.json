{
  "include": [
    "**/*",
    "./.hono/types/**/*"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "rootDirs": [
      "."
    ],
    "paths": {
      "+types/*": [
        "./.hono/types/src/*"
      ]
    },
    "lib": [
      "ESNext"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
}